          fn_adsp_bpwueZuVnQc4ykIqBZeC.setCenterFlash = function(v) {
            var div = document.getElementById("adsp_content_right_div");
            if (div != null) {
              div.style.display = v;
            }
          }
          // 删除之前的对象
