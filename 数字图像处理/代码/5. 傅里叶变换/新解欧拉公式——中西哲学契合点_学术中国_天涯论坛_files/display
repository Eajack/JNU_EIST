          var temp = document.createElement("div");
          temp.appendChild(bar);
          document.write(temp.innerHTML);

          var div_right = document.createElement("div");
              div_right.innerHTML = '<a href="http://807.tianya.cn/count?t=UTsJQaNbb3ppEk8mdLkf&backurl=http://s.click.taobao.com/t?e=m%3D2%26s%3DATmm%2FgrRSHccQipKwQzePDAVflQIoZepK7Vc7tFgwiFRAdhuF14FMS96aS5YzDiG1aH1Hk3GeOjjg1WMqTPPvKb%2FH0T7w4j3lgu4%2ByB8EaOiZ%2BQMlGz6FQ%3D%3D" target="_blank"><img width="120" height="500" src="http://801.tianyaui.com/res/2013/1105/1383639623937.gif" border="0"></a>';
          var closeLink = document.createElement("img");
          closeLink.src = "http://static.tianyaui.com/img/static/2008/tygg/c120.jpg";
          closeLink.setAttribute("id", "adsp_content_right_close_btn");
          closeLink.style.cursor = "pointer";
          closeLink.onclick = function () { fn_adsp_bpwueZuVnQc4ykIqBZeC.showRandDiv("none"); };      



          var master = document.createElement("div");
          master.style.display = "none";
          master.appendChild(div_right);
          master.appendChild(closeLink);
          divs.push(master);

          bar = document.getElementById("float_right_parent_div");
          bar.appendChild(master);

          var rnd_idx = Math.floor(Math.random() * (divs.length));

          fn_adsp_bpwueZuVnQc4ykIqBZeC.changeplay = function(v) {
            var div = null;
            if (rnd_idx >= divs.length) {
              if (divs.length % 2 == 0) {
                rnd_idx = Math.floor(Math.random() * (divs.length));
              } else {
                rnd_idx = rnd_idx % divs.length;
              }
            }
            for (var jdx = 0; jdx < divs.length; jdx++) {
              divs[jdx].style.display = "none";
            }
            div = divs[rnd_idx];
            div.style.display = "block";
            rnd_idx++;
          }
          fn_adsp_bpwueZuVnQc4ykIqBZeC.changeplay();
          setInterval(fn_adsp_bpwueZuVnQc4ykIqBZeC.changeplay, 25000);

}
} catch (e) {}}
