function fn_adsp_9vhIYRtWPGtIt6eFEDRQ() {try {
    // init
    String.prototype.endsWith = function(suffix) {
        return this.indexOf(suffix, this.length - suffix.length) !== -1;
    };
    var elems = new Array();
    var max_count = parseInt("1");
    var count = 0;

    var canvas = document.getElementById('q_banner_950_90_4');
    //_nodispaly="true"
    if (jQuery("#q_banner_950_90_4").css("display") == "none" && jQuery("#q_banner_950_90_4").attr("_nodispaly") != "true") {
      jQuery("#q_banner_950_90_4").css("display", "block");
    }
    var a_target='';
    var window_target='';
      a_target=' target="_blank" ';


    // fill data
    elems.push({"title":"4","res":"http://801.tianyaui.com/2013/baidu/tonglang4.html","url":"http://807.tianya.cn/count?t=ZzmCnLkF8HKLBzKuZFxy&backurl="});
    count ++;

    // show
    var rnd_idx = Math.floor(Math.random() * (elems.length));
    var elm = elems[rnd_idx];

    if ( elm.res.endsWith("jpg") || elm.res.endsWith("gif") || elm.res.endsWith("png") ) {
      if ( elm.url == "" ) {
          canvas.innerHTML = '<div class="qing_ad_hd"></div><IMG width="950" height="90" src="' + elm.res + '" border="0"><div class="qing_ad_bd"></div>';
      } else {
          canvas.innerHTML = '<div class="qing_ad_hd"></div><A href="' + elm.url + '" ' + a_target + '><IMG width="950" height="90" src="' + elm.res + '" border="0"></A><div class="qing_ad_bd"></div>';
      }
    } else if ( elm.res.endsWith("swf") ) {
      if ( elm.url == "" ) {
        canvas.innerHTML = '<div class="qing_ad_hd"></div><EMBED id="q_banner_950_90_4_swf" width="950" height="90" wmode="opaque" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" quality="High" src="' + elm.res + '"><div class="qing_ad_bd"></div>';
      } else {
        canvas.innerHTML = '<div class="qing_ad_hd"></div><div style="position: relative;width:950px;height:90px;background-color:#F5F9FA"><embed id="q_banner_950_90_4_swf" style="position:absolute;z-index:0;left:0;" src="' + elm.res + '"quality="high" width="950" height="90" pluginspage="http://www.macromedia.com/go/getflashplayer"type="application/x-shockwave-flash" wmode="opaque"><a href="' + elm.url + '" target="_blank" style="cursor:pointer"><div style="position:relative;filter:alpha(opacity=0);left:0;top:0;width:950px;height:90px;z-index:10;opacity:0;background-color:#000000"></div></a></div><div class="qing_ad_bd"></div>';
      }
    } else {
      canvas.innerHTML ='<iframe width="950" height="90" hspace=0 vspace=0 frameborder=0 marginheight=0 marginwidth=0 scrolling=no src="' + elm.res + '"  name="4"></iframe>';
    }

} catch (e) {}}
